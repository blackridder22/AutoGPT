{
  "manifest_version": 3,
  "name": "Auto GPT Beta",
  "version": "0.2.0",
  "description": "A powerful Auto GPT that supports multiple models through OpenRouter.",
  "permissions": [
    "sidePanel",
    "storage",
    "contextMenus"
  ],
  "host_permissions": [
    "https://*.supabase.co/*",
    "https://openrouter.ai/*"
  ],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "action": {
    "default_title": "Auto GPT",
    "default_icon": {
      "16": "icons/AGPT16.png",
      "48": "icons/AGPT48.png",
      "128": "icons/AGPT128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/AGPT16.png",
    "48": "icons/AGPT48.png",
    "128": "icons/AGPT128.png"
  }
}